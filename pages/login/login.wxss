/**login.wxss**/
.fl{
  float: left;
}
.fr{
  float: right;
}
.oh{
  overflow: hidden;
}
.top{
  width: 100%;
  height: 340rpx;
  position: relative;
}
.top image{
  width: 100%;
  display: block;
  position: absolute;
  top: 0;
  right: 0;
}
.btn{
  width: 295rpx;
  position: absolute;
  bottom: -2px;
  left: 0;
  right: 0;
  margin: auto;
  display: flex;
}
.tab_btn{
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  line-height: 60rpx;
  color: white;
}
.tab_btn_active{
  color: #ffad39;
  border-bottom: 2px solid #ffad39;
}
.my_ipt{
  width: 600rpx;
  margin: 0 auto;
  margin-top: 45rpx;
}
.my_ipt view{
  width: 100%;
  overflow: hidden;
  margin-top: 55rpx;
  padding-bottom: 24rpx;
  border-bottom: 1px solid #cfcfcf; 
}
.my_ipt view image{
  display: block;
  width: 27rpx;
  height: 35rpx;
  margin: 15rpx 30rpx 0rpx 30rpx;
}
.my_ipt view input{
  display: block;
  width: 305rpx;
  height: 35rpx;
  line-height: 35rpx;
  font-size: 30rpx;
  margin-top: 7rpx;
  border: 0;
  outline: none;
}
.yzm{
  width: 190rpx;
  height: 59rpx;
  line-height: 59rpx;
  text-align: center;
  font-size: 28rpx;
  color: #ff9e35;
  border: 1px solid #ff9f35;
  border-radius: 25px;
  -webkit-border-radius: 25px;
  -moz-border-radius: 25px;
}
.lg_btn{
  width: 600rpx;
  margin: 0 auto;
  margin-top: 35rpx;
}
.sub_lg{
  width: 100%;
  height: 89rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
}
.lg_btn text{
  font-size: 28rpx;
  color: #333333;
  line-height: 70rpx;
}
.line{
  width: 100%;
  height: 19rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  margin-top: 70rpx;
}
/* 发送验证码按钮隐藏，并展示倒数60s提示 */
.hide{
 display: none;
}
.show{
 display: inline-block;
}
