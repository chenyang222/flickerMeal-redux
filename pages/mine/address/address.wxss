page{
    width: 100%;
    height: 100%;
  }
  .header{
     background-color: #ffffff;
    padding: 0 25rpx;
    line-height: 88rpx;
    height: 88rpx;
    display: flex;
    justify-content: space-between;
  }
  .header .back{
    width: 20rpx;
    height: 34rpx;
  }
  .header .add{
    width: 34rpx;
    height: 34rpx;
  }
  .header image{
    height: 100%;
    width: 100%;
  }
  
  .list{
    height: 100%;
    overflow-y: scroll;
    padding-top: 2rpx;
    width: 100%;
   
  }
  .item{
    margin-top: 2rpx;
    background-color: #ffffff;
    width: 100%;
    height: 185rpx;
    display: flex;
    justify-content: space-between;
  }
   .item .itemLeft{
    width: 88rpx;
  }
  .item .itemLeft image{
    width: 23rpx;
    height: 32rpx;
    padding: 77rpx 35rpx ;
  }
  .item .itemCenter{
    flex: 1;
  }
   .item .itemCenter .itemCenterAddress{
    max-width:553rpx;
    overflow:hidden; 
    white-space:nowrap; 
    text-overflow:ellipsis;
    font-size: 27rpx;
    margin-top: 28rpx;
  }
   .item .itemCenter .itemCenterName{
    font-size: 24rpx;
    margin-top: 20rpx;
  }
   .item .itemCenter .itemCenterDefalut{
    display: flex;
    margin-top: 26rpx;
    font-size: 21rpx;
    width: 100%;
  }
   .item .itemCenter .itemCenterDefalut image{
    margin-top: 3rpx;
    width: 24rpx;
    height: 24rpx;
  }
   .item .itemRight{
    width: 88rpx;
   
  }
   .item .itemRight image{
     width: 28rpx;
     height: 28rpx;
     padding: 80rpx 28rpx ;
  }
  
  
  
  .touch-item {
   font-size: 14px;
   display: flex;
   justify-content: space-between;
   border-bottom:1px solid #ccc;
   width: 100%;
   overflow: hidden;
   height: 185rpx;
  }
  .content {
   width: 100%;
   /* padding: 10px;
   line-height: 22px; */
   margin-right:0;
   -webkit-transition: all 0.4s;
   transition: all 0.4s;
   -webkit-transform: translateX(90px);
   transform: translateX(90px);
   margin-left: -90px
  }
  .del {
   background-color: orangered;
   width: 90px;
   display: flex;
   flex-direction: column;
   align-items: center;
   justify-content: center;
   color: #fff;
   -webkit-transform: translateX(90px);
   transform: translateX(90px);
   -webkit-transition: all 0.4s;
   transition: all 0.4s;
  }
  .touch-move-active .content,
  .touch-move-active .del {
   -webkit-transform: translateX(0);
   transform: translateX(0);
  }
