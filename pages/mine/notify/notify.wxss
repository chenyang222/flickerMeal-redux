/* pages/mine/msgnotification/msgnotification.wxss */
.msgnotification{
    width: 100%;
  }
  .msglist{
    width: 100%;
    margin-top: 20rpx;
  }
  .msgitem{
    width: 100%;
    height: 178rpx;
    border-bottom: 1rpx solid #DDDDDD;
    display: flex;
    flex-direction: column;
    position: relative;
    background-color: #ffffff;
  }
  .msgitem text:nth-of-type(1){
    font-size: 28rpx;
    color: #333333;
    margin-top: 27rpx;
    margin-left: 43rpx;
  }
  .msgitem text:nth-of-type(1) text{
    width: 14rpx;
    height: 14rpx;
    background-color: #FE7F2C;
    border-radius: 50%;
    display: inline-block;
    margin: 0;
    margin-right: 15rpx;
  }
  .msgitem text:nth-of-type(2){
    font-size: 22rpx;
    color: #666666;
    margin-top: 15rpx;
    margin-left: 43rpx;
  }
  .msgitem text:nth-of-type(3){
    font-size: 22rpx;
    color: #999999;
    margin-top: 20rpx;
    margin-left: 43rpx;
  }
  .msgitem text:nth-of-type(4){
    font-size: 20rpx;
    color: #666666;
    position: absolute;
    right: 29rpx;
    bottom: 31rpx;
  }