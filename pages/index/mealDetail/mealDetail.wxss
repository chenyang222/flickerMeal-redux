/* pages/index/mealDetail/mealDetail.wxss */
.share{
  width:29rpx;
  height:33rpx; 
  position: absolute;
  right:25rpx;
  top:35rpx;
}
.lunbo{
  width: 100%;
}
.lunbo image{
  width: 100%;
}
.car-icon{
  width:25rpx;
  height:25rpx; 
}
.addCar{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 40rpx 24rpx;
}
.meal-name{
  font-size:30rpx;
  font-family:SourceHanSansCN-Medium;
  color:rgba(45,45,45,1);
}
.zhe-price{
  font-size:38rpx;
  font-family:SourceHanSansCN-Medium;
  color:rgba(245,65,60,1);
}
.yuanjia{
  font-size:28rpx;
  font-family:SourceHanSansCN-Regular;
  color:rgba(102,102,102,1);
  text-decoration: line-through;
}
.money-icon{
  font-size:28rpx;
  font-family:SourceHanSansCN-Medium;
  color:rgba(245,65,60,1);
}
.addcar-btn{
  background-size: 100% 100%;
  width:200rpx;
  height:74rpx;
  display: inline-block;
  margin-top: 10rpx;
  background-image: url(data:image/png;base64,);
}
.meal-detail{
  padding: 20rpx 24rpx;
  background-color: #fff;
}
.meal-title-icon{
  display: inline-block;
  width:2rpx;
  height:27rpx; 
  background:rgba(255,158,53,1);
  margin-right: 20rpx;
}
.meal-title-txt{
  font-size:30rpx;
  font-family:SourceHanSansCN-Medium;
  color:rgba(102,102,102,1);
}
.meal-title-detial-txt{
  font-size:24rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(124,124,124,1);
}
.caipin{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  
  padding: 20rpx 24rpx;
  background-color: #fff;
  font-size:24rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(124,124,124,1);
}
.shicai{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 20rpx 24rpx;
  background-color: #e4e4e4;
  font-size:24rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(124,124,124,1);
}
.zhishu-xing image{
  width:24rpx;
  height:23rpx; 
}
.hexu{
  border-top: 1px dashed #cacaca;
}
.ping-list-img-wrap{
  overflow: hidden;
  border-radius: 50rpx;
  width:12%;
  height:90rpx; 
}
.ping-list-img{
  width:90rpx;
  height:90rpx; 
}
.ping-list-right{
  font-size:24rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(124,124,124,1);
  width: 86%;
  padding-left: 15rpx;
}
.list-row{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.ping-list{
  padding: 30rpx 25rpx;
  overflow: hidden;
  background-color: #fff;
  border-bottom: 1px dashed #cacaca;
}
.haoping-xing image{
  width:24rpx;
  height:23rpx; 
}
.ping-img{
  display: flex;
  flex-direction: row;
}
.ping-img image{
  width: 140rpx;
  height: 105rpx;
  flex-grow: 1;
  margin-right: 10rpx;
}
.zhankai-btn{
  font-size:24rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(255,158,53,1);
}