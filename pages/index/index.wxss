/**index.wxss**/
/* top-header */
.top-header{
  width: 100%;
  height: 220rpx;
  padding: 0rpx 20rpx;
  position: fixed;
  top: 0;
  left: -20rpx;
  background-size: 100% 100%;
  z-index: 999;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.top-header .location{
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  margin-top: 20rpx;
}
.top-header .location .locationIcon{
  width:23rpx;
  height:32rpx;
}
.top-header .location text{
  color: #fff;
  font-size: 28rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
}
.top-header .location .locationSelect{
  width:18rpx;
  height:14rpx;
}
.top-header .search{
  width:90%;
  margin-left: 30rpx;
  margin-bottom: 20rpx;
  height:60rpx;
  background:rgba(255,255,255,1);
  border-radius: 30rpx;
  display: flex;
  align-items: center;
}
.top-header .search .searchIcon{
  width:28rpx;
  height:28rpx; 
  margin-left:15rpx;
  margin-right: 15rpx;
}
.top-header .search .searchInput{
  font-size:24rpx;
  color:rgba(152,152,152,1);
  line-height:33rpx; 
}

/* banner */
.banner{
  width: 100%;
  margin-top: 220rpx;
}
.slide-image{
  width: 750rpx;
}

/* 优惠券 */
.couponPart{
  width: 94%;
  margin: 0 auto;
  margin-top: 15rpx;
  display: flex;
  /* justify-content: space-between; */
  flex-wrap: wrap;
}
.couponBox{
  width: 32%;
  position: relative;
  margin-left:1.33%;

}
.couponBox .bg{
  width: 100%;
}
.couponBox .couponPrice{
  position: absolute;
  bottom: 5%;
  left: 5%;
}
.couponBox .price{
  font-size:45rpx;
  font-family:FZLTDHJW--GB1-0;
  color:rgba(254,254,252,1);
}
.couponBox .unit{
  font-size:21rpx;
  font-family:PingFangSC-Medium;
  color:rgba(254,254,252,1);
  margin-left: 5rpx;
}
.couponBox .nameBtn{
  position: absolute;
  top: 15%;
  right: 5%;
  display: flex;
  flex-direction: column;
  align-items:flex-end;

}
.couponBox .name{
  font-size:21rpx;
  font-family:MFYueYuan_Noncommercial-Regular;
  color:rgba(254,254,252,1);
}
.couponBox .btn{
  width:88rpx;
  height:29rpx; 
  font-size:20rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(249,78,58,1);
  background-color: rgba(254,254,252,1);
  border-radius: 15rpx;
  line-height: 29rpx;
  text-align: center;
  margin-top: 10rpx;
}
.indexMachine{
  background-color: #f8f8f8;
  padding: 30rpx 0rpx;
}
.isCanGet{
  width: 60%;
  padding: 0 5% 0 35%;
  background-color: #ffffff;
  text-align: right;
  margin-right: 3%;
  font-size:24rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(102,102,102,1);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.isCanGet view{
  display: flex;
  align-items: center;
}
.isCanGet .dw{
  width:30rpx;
  height:40rpx;
  margin-right: 15rpx;
}
.isCanGet .pullDown{
  width:20rpx;
  height:9rpx;
}

/* 推荐餐品 */
.recomendPart{
  width: 100%;
  padding: 10rpx 0rpx;
  background:rgba(255,255,255,1);
}
.recomendPart .recomentHeader{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.recomendPart .recomentHeader view{
  display: flex;
  align-items: center;
}
.recomendPart .recomentHeader .line{
  width:3rpx;
  height:28rpx;
  margin-left: 25rpx;
  margin-right: 15rpx;
}
.recomendPart .recomentHeader .recomendTxt{
  font-size:30rpx;
  font-family:PingFangSC-Medium;
  color:rgba(45,45,45,1);
}
.recomendPart .recomentHeader .moreTxt{
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  color:rgba(102,102,102,1);
}
.recomendPart .recomentHeader .moreIcon{
  width:13rpx;
  height:23rpx;
  margin-left: 15rpx;
  margin-right: 25rpx;
}
.recomendPart .recomentContent{
  display: flex;
}
.recomendPart .recomentContent .recomentBox{
  width: 29%;
  padding: 2%;
  position: relative;
  margin-left: 0.333%;
}
.recomendPart .recomentContent .recomentBox .picture{
  width:210rpx;
  height:140rpx;
  border-radius: 5rpx;
}
.recomendPart .recomentContent .recomentBox .productIcon{
  top: 5%;
  left: 5%;
}
.recomendPart .recomentContent .recomentBox .reName{
  font-size:28rpx;
  font-family:PingFangSC-Medium;
  color:rgba(45,45,45,1);
  margin-top: 20rpx;
}
.recomendPart .recomentContent .recomentBox .recomentNum{
  font-size:20rpx;
  font-family:PingFangSC-Regular;
  color:rgba(151,151,151,1);
}
.recomendPart .recomentContent .recomentBox .addGoods{
  display: flex;
  justify-content: space-between;
}
.recomendPart .recomentContent .recomentBox .price{
  font-size:28rpx;
  font-family:PingFangSC-Medium;
  color:rgba(253,106,8,1);
}
.recomendPart .recomentContent .recomentBox .add{
  width:33rpx;
  height:33rpx;
  display: block;
  margin-right: 15rpx;
}

/* 滑动tab */
.tab-h{
  height: 80rpx;
  width: 100%;
  background: #F7F7F7;
  font-size: 16px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tab-item{
  width: 30.3%;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  height: 77rpx;
  margin-left: 1.5%;
  margin-right: 1.5%;
}
.tab-item image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
.tab-h .active{
  border-bottom: 1px solid #FFC240;
  color: #FFC240;
}
.tabCtnL{
  width: 19.3%;
  font-size:22rpx;
  font-family:PingFangSC-Regular;
  color:rgba(98,98,98,1);
  display: flex;
  flex-direction: column;
  background-color: #f0efef;
  margin-right: 30rpx;
}
.tabCtnL text{
  text-align: center;
  padding: 35rpx 0;
}
.tabCtnR{
  width: 76%;
  overflow-y: auto;
}
.jinrigouTypeSele{
  background:rgba(255,255,255,1);
}

.xingqi {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  background-color: #f8f8f8;
  padding:30rpx 0;
}
.xingqi .weekActive{
  background-color: #ffffff;
}
.xingqi .xingqiList{
  display: flex;
  flex-direction: column;
  font-size:29rpx;
  font-family:SourceHanSansCN-Regular;
  color:rgba(45,45,45,1);
  width:15%;
  padding: 1%;
  margin-top: 15rpx;
}
.xingqi .xingqiList text{
  width: 100%;
  text-align: center;
}
.xingqi .xingqiList .date{
  font-size:24rpx;
  font-family:SourceHanSansCN-Regular;
  color:rgba(102,102,102,1);
}

.noMachine{
  text-align: center;
  margin-top: 50rpx;
}

.manjianPop{
  position: fixed;
  bottom: 0;
  font-size:22rpx;
  font-family:PingFangSC-Regular;
  color:rgba(255,255,255,1);
  text-align: center;
  background: rgba(0,0,0,.5);
  width:90%;
  padding:10rpx 5%;
  z-index: 999;
}

.jia {
  width: 41rpx;
  height: 41rpx;
  float: left;
}

.jian {
  width: 41rpx;
  height: 41rpx;
  float: left;
}