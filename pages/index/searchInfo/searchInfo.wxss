
.car-title{
  padding: 25rpx;
  /* position: relative; */
  background-color: #fff;
  margin-bottom: 1rpx;
  position: fixed;
  top: 0;
  z-index: 999;
  width: 93%;
  height: 55rpx;
  border-bottom: 1px solid #f7f7f7;
}
.search-kuang{
  background: #f3f0f0;
  width: 70%;
  /* height: 30rpx; */
  line-height: 30rpx;
  border-radius: 50rpx;
  padding: 0rpx 20rpx;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
}
.search-img{
  width:28rpx;
  height:28rpx;
  margin-top: 12rpx;
  margin-right: 10rpx;
}
.search-input{
  width: 90%;
  display: inline-block;
  font-size:24rpx;
  font-family:SourceHanSansCN-Normal;
  color:rgba(152,152,152,1);
}
.car-title text{
  display: inline-block;
  width: 100%;
  text-align: center;
}


.searchList .product{
  margin-top: 0rpx;
}


